<!DOCTYPE html>
<html>
<head>
  <title>WeThePac</title>
  <%= stylesheet_link_tag  'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link href='http://fonts.googleapis.com/css?family=Slabo+27px' rel='stylesheet' type='text/css'>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/3.0.0/handlebars.js"></script>
</head>
<body>
<% if current_user %>
  <%= render file: "/layouts/_logged_in_header.html.erb" %>
<% else %>
  <%= render file: "/layouts/_logged_out_header.html.erb" %>
<% end %>
<%= csrf_meta_tag %>
<div class="content">
  <%= yield %>
</diV>

</body>
</html>
